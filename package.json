{
  "name": "cardealer",
  "version": "1.25.001",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "postinstall": "patch-package",
    "build": "yarn react-native build-android --mode release",
    "setup": "sh init.sh",
    "clean": "sh clean.sh",
    "prints:iphone": "nohup sh checkBuildForPrints.sh --ios; detox test --configuration iphone.debug e2e/iphone.test.js && sh organize.sh iphone && xcrun simctl shutdown all",
    "prints:ipad": "nohup sh checkBuildForPrints.sh --ios; detox test --configuration ipad.debug e2e/ipad.test.js && sh organize.sh ipad && xcrun simctl shutdown all",
    "prints:phone": "sh checkBuildForPrints.sh --android; detox test --configuration android.debug e2e/android.test.js --headless && sh organize.sh android adb emu kill adb start-server",
    "prints:tablet": "sh checkBuildForPrints.sh --android; detox test --configuration tablet.debug e2e/tablet.test.js --headless && sh organize.sh tablet adb emu kill adb start-server",
    "prints": "yarn prints:iphone && yarn prints:ipad && yarn prints:phone && yarn prints:tablet"
  },
